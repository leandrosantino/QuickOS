{
  "name": "quick_os",
  "version": "0.1.0",
  "private": true,
  "main": "build/main.js",
  "author": {
    "name": "Leandro",
    "email": "leandrosantino2013@gmail.com"
  },
  "homepage": "./",
  "description": "Aplicativo de gestão da manutenção",
  "scripts": {
    "react": "craco start",
    "react:build": "craco build ",
    "electron:build": "set NODE_ENV=production && webpack --progress",
    "electron:build:dev": "set NODE_ENV=development && webpack --progress ",
    "app": "cls && npm run electron:build:dev && cls && electron . && cls",
    "dist": "craco build && npm run electron:build && electron-builder -w -c.extraMetadata.main=build/main.js",
    "server": "tsx watch ./server/index.ts",
    "db.studio": "npx prisma studio",
    "db.seed": "tsx prisma/seed/seed.ts",
    "db.migrate": "npx prisma migrate dev",
    "test": "jest"
  },
  "prisma": {
    "seed": "npm run db.seed"
  },
  "build": {
    "asar": true,
    "appId": "com.teste.electron",
    "productName": "QuickOS",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "database/**/*",
      "theme.js"
    ],
    "asarUnpack": [
      "database/app.db",
      "public/views/serviceOrder.ejs"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "./src/icon.png"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.22.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@trpc/client": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "chalk": "^5.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "ejs": "^3.1.8",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-inlinesvg": "^3.0.1",
    "react-scripts": "5.0.1",
    "react-switch": "^7.0.0",
    "react-toastify": "^9.1.1",
    "recharts": "^2.2.0",
    "trpc-playground": "^1.0.4",
    "web-vitals": "^2.1.4",
    "zod": "^3.20.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.4.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "autodetect-decoder-stream": "^2.0.2",
    "craco": "^0.0.3",
    "csv-parse": "^5.3.5",
    "csv-reader": "^1.0.11",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prisma": "^4.9.0",
    "prisma-erd-generator": "^1.2.5",
    "shelljs": "^0.8.5",
    "tailwind-scrollbar": "^2.0.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "tsx": "^3.12.2",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
