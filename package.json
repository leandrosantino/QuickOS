{
  "name": "quick_os",
  "version": "0.1.0",
  "private": true,
  "main": "build/main.js",
  "author": {
    "name": "Leandro",
    "email": "leandrosantino2013@gmail.com"
  },
  "homepage": "./",
  "description": "tes tes",
  "scripts": {
    "react": "react-scripts start",
    "react:build": "react-scripts build",
    "electron:build": "tsc -p ./electron.tsconfig.json",
    "electron:watch": "tsc -w -p ./electron.tsconfig.json",
    "app": "cls && npm run electron:build && cls && electron . && cls",
    "dist": "npm run dbmigrate && npm run react:build && npm run electron:build && electron-builder -w -c.extraMetadata.main=build/main.js",
    "test": "",
    "eject": "react-scripts eject",
    "server": "tsx watch ./electron/server/index.ts",
    "dbstudio": "npx prisma studio",
    "seed": "tsx prisma/seed/seed.ts",
    "dbmigrate": "npx prisma migrate dev"
  },
  "prisma": {
    "seed": "npm run seed"
  },
  "build": {
    "asar": true,
    "appId": "com.teste.electron",
    "productName": "QuickOS",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "database/**/*",
      "theme.js"
    ],
    "asarUnpack": [
      "database/app.db",
      "public/views/serviceOrder.ejs"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "./src/icon.png"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.22.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@trpc/client": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "chalk": "^5.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "ejs": "^3.1.8",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-inlinesvg": "^3.0.1",
    "react-scripts": "5.0.1",
    "react-switch": "^7.0.0",
    "react-toastify": "^9.1.1",
    "recharts": "^2.2.0",
    "trpc-playground": "^1.0.4",
    "typescript": "^4.9.3",
    "web-vitals": "^2.1.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@types/cors": "^2.8.13",
    "autodetect-decoder-stream": "^2.0.2",
    "csv-parse": "^5.3.5",
    "csv-reader": "^1.0.11",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "http-url-builder": "^1.1.0",
    "prisma": "^4.9.0",
    "prisma-erd-generator": "^1.2.5",
    "tailwind-scrollbar": "^2.0.1",
    "tsx": "^3.12.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
